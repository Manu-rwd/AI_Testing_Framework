qa-framework/tools/poppler/bin/*.exe filter=lfs diff=lfs merge=lfs -text
qa-framework/tools/poppler/bin/*.dll filter=lfs diff=lfs merge=lfs -text
# Source uses LF
*.ts  text eol=lf
*.tsx text eol=lf
*.md  text eol=lf
*.json text eol=lf
*.yml text eol=lf
*.yaml text eol=lf

# Generated CSVs must stay CRLF for Excel; keep them as-is and UTF-8
exports/*.csv -text eol=crlf
* text=auto eol=lf
*.js  text eol=lf
*.jsx text eol=lf
*.csv text eol=lf
qa-framework/tools/pandoc/pandoc.exe filter=lfs diff=lfs merge=lfs -text
